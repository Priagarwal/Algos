
public class PriorityQueue {
	Edge a[];
	int heapsize;
	int MAX;
	
	int parent(int i) {
		if ( i > 0 && i < heapsize) {
			return((i - 1)/2);
		}
		return -1;
	}
	
	int lchild(int i) {
		if ( i > 0 && i < heapsize) {
			int l = 2*i + 1;
			if (heapsize > l)
			return l;
		}
		return -1;
	}
	
	int rchild(int i) {
		if ( i > 0 && i < heapsize) {
			int l = 2*i + 2;
			if (heapsize > l)
			return l;
		}
		return -1;
	}
	
	boolean isEmpty() {
		return(heapsize == 0);
	}
	
	Edge deleteMin() {
		Edge val = a[0];
		
		return null;
	}
	
	public PriorityQueue() {
		// TODO Auto-generated constructor stub
	}

}
