
public class PQ {
	int a[];
	int n;
	int heapsize;
	
	public PQ(int n) {
		this.n = n;
		a = new int[n];
	}
	
	public void insert(int s) {
		if (this.heapsize < this.n) {
			a[heapsize] = s;
			this.heapsize++;
		}
	}
	
